<div class="x_panel">
    <div class="x_title">
        <h2>Last Update date for Nagios tables</h2>
        <a role="button" class="collapse-link pull-right"><i class="fa fa-chevron-up"></i></a>


        <div class="clearfix"></div>
    </div>
    <div class="x_content">
        {% if error is defined %}
            <p class="alert alert-danger">{{ error }}</p>
        {% else %}
            <table style="display: none" id="lastUpdateTable" class="table">
                <thead>
                <tr>
                    <th>Table</th>
                    <th>Last Update</th>

                </tr>
                </thead>
                <tbody>
                {% for key,tab in tabLastUpdate %}

                    <tr>
                        <td>{{ key }}</td>
                        <td>{{ tab|date("d M Y h:m:s") }}</td>
                    </tr>

                {% endfor %}
                </tbody>
            </table>
        {% endif %}

    </div>
</div>