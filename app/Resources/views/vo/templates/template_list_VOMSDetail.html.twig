{% if  serial is defined and voms is defined %}

    <tr id="vomsDetail{{ serial }}{{ host }}" style="display: none">

        {% for voms in voms %}
            <td colspan="{{ colspan }}">
                <ul>
                    <li><strong>Host : </strong>{{ voms.hostname }}</li>
                    <li><strong>List members url : </strong><a href="{{ voms.members_list_url }}">{{ voms.members_list_url }}</a></li>
                    <li><strong>DN : </strong>{{ voms.dn }}</li>
                    <li><strong>CA_DN : </strong>{{ voms.ca_dn }}</li>
                    <li><strong>Public Key : </strong>{{ voms.cert }}</li>

                </ul>
            </td>
        {% endfor %}
    </tr>

{% else %}

    <tr id="vomsDetailError" style="display: none">
        <td colspan="8">
            {{ error }}
        </td>

    </tr>
{% endif %}

